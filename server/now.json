{
  "version": 2,
  "name": "auth-from-scratch",
  "env": {
    "TOKEN_SECRET": "@auth-jwt-secret",
    "DEFAULT_ADMIN_PASSWORD": "@auth-admin-pwd",
    "DB_URL": "@auth-scratch-db"
  },
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "src/index.js"
    }
  ],
  "alias": [
    "auth-from-scratch"
  ]
}